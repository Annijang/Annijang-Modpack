{
	// 设置宠物GUI界面的标题。
	"followerListHeaderFormat": "Followers:",
	// 如果设置为true，那么会在宠物GUI界面中显示宠物的生命值。
	"showFollowerHealth": true,
	// 设置宠物GUI界面中的生命值显示格式。<health>里的内容会以百分比的形式渲染。
	"followerHealthFormat": ": <health>%",
	// 如果设置为true，那么在宠物GUI界面中会显示宠物和玩家之间的距离，单位是块。
	"showFollowerDistance": true,
	// 设置宠物GUI界面中的距离显示格式。<distance>里的内容会以块为单位渲染。
	"followerDistanceFormat": " (<distance> blocks)",
	// 设置检测玩家附近非坐立状态和非驯服的宠物距离，如果设置为-1，那么会禁用该功能。
	// 范围：-1 ~ 300
	"distanceToCheckForFollowersAround": 50,
	// 设置检测范围内非坐立和非驯服状态宠物的间隔时间，单位是秒。
	// 范围：0 ~ 3600
	"timeBetweenChecksInSeconds": 2,
	// 如果设置为true，那么会将宠物GUI的位置设置到左侧。
	"followerListPositionIsLeft": true,
	// 如果设置为true，那么会将宠物GUI的位置设置到屏幕中央。
	"followerListPositionIsCenter": false,
	// 如果设置为true，那么会将宠物GUI的位置设置到右侧。
	"followerListPositionIsRight": false,
	// 设置宠物GUI的Y轴偏移坐标，防止与其他模组的图标重叠。
	// 范围：0 ~ 3000
	"followerListHeightOffset": 20,
	// 如果设置为true，那么会在文本后方渲染一层阴影。
	"drawTextShadow": true,
	// 设置文本的红色RGB值。
	// 范围：0 ~ 255
	"RGB_R": 255,
	// 设置文本的绿色RGB值。
	// 范围：0 ~ 255
	"RGB_G": 255,
	// 设置文本的蓝色RGB值。
	// 范围：0 ~ 255
	"RGB_B": 255
}