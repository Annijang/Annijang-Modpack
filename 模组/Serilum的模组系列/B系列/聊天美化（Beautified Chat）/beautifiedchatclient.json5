{
	// 变量：%timestamp% = 聊天栏中信息的发送时间。 %username% = 发送聊天信息的玩家名称。 %chatmessage% = 玩家在聊天栏发送的聊天信息。
	"chatMessageFormat": "%timestamp% | %username%: %chatmessage%",
	// 设置在聊天栏中时间的显示格式。*=默认。 *（HH:mm） = 21:00，（HH:mm:ss） = 21:00:05，（hh:mm a） = 9:00 PM
	"timestampFormat": "HH:mm",
	// 设置聊天栏中时间颜色，可用的值如下。0：黑色，1：湖蓝色，2：深绿色，3：深黑色，4：深红色，5：深紫色，6：金色，7：灰色，8：深灰色，9：蓝色，10：绿色，11：天蓝色，12：红色，13：粉红色，14：黄色，15：白色。
	// 范围：0 ~ 15
	"chatTimestampColour": 8,
	// 设置聊天栏中玩家名称的颜色，可用的值如下。0：黑色，1：湖蓝色，2：深绿色，3：深黑色，4：深红色，5：深紫色，6：金色，7：灰色，8：深灰色，9：蓝色，10：绿色，11：天蓝色，12：红色，13：粉红色，14：黄色，15：白色。
	// 范围：0 ~ 15
	"chatUsernameColour": 2,
	// 设置聊天栏中的聊天信息的颜色，可用的值如下。0：黑色，1：湖蓝色，2：深绿色，3：深黑色，4：深红色，5：深紫色，6：金色，7：灰色，8：深灰色，9：蓝色，10：绿色，11：天蓝色，12：红色，13：粉红色，14：黄色，15：白色。
	// 范围：0 ~ 15
	"chatMessageColour": 15,
	// 设置来自“chatMessageFormat”中的其他符号的颜色，除了变量，可用的值如下。0：黑色，1：湖蓝色，2：深绿色，3：深黑色，4：深红色，5：深紫色，6：金色，7：灰色，8：深灰色，9：蓝色，10：绿色，11：天蓝色，12：红色，13：粉红色，14：黄色，15：白色。
	// 范围：0 ~ 15
	"chatOtherSymbolsColour": 7
}